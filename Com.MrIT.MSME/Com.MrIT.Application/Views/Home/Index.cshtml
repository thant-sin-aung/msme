@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout_Public.cshtml";
}



<div class="row">
    <div class="col-md-12">
        <div class="card">

            <!-- /.card-header -->
            <div class="card-body">
                <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                    </ol>
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="d-block w-100" src="https://msmewebportal.gov.mm/img/slider1.jpg" alt="First slide">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="https://msmewebportal.gov.mm/img/slider2.jpg" alt="Second slide">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="https://msmewebportal.gov.mm/img/slider4.jpg" alt="Third slide">
                        </div>
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
            <!-- /.card-body -->
        </div>
        <!-- /.card -->
    </div>
</div>
<div class="row">
    <div class="col-md-4 text-center">
        <a style="width:350px;" href="https://msmewebportal.gov.mm/contents/registration-procedures" class="btn btn-info btn-lg">
            <div class="box-part text-center">
                <div class="box-title">
                    <h4>Registration Procedures</h4>
                </div>

            </div>
        </a>
    </div>
    <div class="col-md-4  text-center">
        <a style="width:350px;" href="https://msmewebportal.gov.mm/contents/requirements-for-registration" class="btn btn-info btn-lg">
            <div class="box-part text-center">
                <div class="box-title">
                    <h4>Requirements for Registration</h4>
                </div>

            </div>
        </a>
    </div>
    <div class="col-md-4  text-center">
        <a style="width:350px;" href="https://msmewebportal.gov.mm/contents/sme-definition" class="btn btn-info btn-lg">
            <div class="box-part text-center">
                <div class="box-title">
                    <h4>SME Definition</h4>
                </div>
            </div>
        </a>
    </div>
</div>
